{
    "type" : "project",
    "license" : "proprietary",
    "require" : {
        "php": "^7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.6@beta",
        "conduction/balancebundle": "dev-master",
        "conduction/commongroundbundle": "dev-symfony51",
        "doctrine/annotations": "^1.0",
        "doctrine/cache": "^1.11",
        "doctrine/doctrine-bundle": "2.3.x-dev",
        "doctrine/doctrine-migrations-bundle": "3.3.x-dev",
        "doctrine/orm": "2.13.x-dev",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^4.3",
        "sensio/framework-extra-bundle": "^5.5",
        "sensiolabs/security-checker": "^6.0",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.18",
        "symfony/framework-bundle": "5.1.*",
        "symfony/mercure-bundle": "*",
        "symfony/property-access": "5.1.*",
        "symfony/property-info": "5.1.*",
        "symfony/proxy-manager-bridge": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/translation": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/yaml": "5.1.*",
        "tbbc/money-bundle": "^4.0",
        "webonyx/graphql-php": "^0.13.8"
    },
    "require-dev" : {
        "api-platform/schema-generator": "^2.1",
        "phpunit/phpunit": "^9",
        "symfony/maker-bundle": "^1.11",
        "symfony/stopwatch": "5.1.*",
        "symfony/web-profiler-bundle": "5.1.*"
    },
    "config" : {
        "preferred-install" : {
            "*" : "dist"
        },
        "sort-packages" : true
    },
    "autoload" : {
        "psr-4" : {
            "App\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "App\\Tests\\" : "tests/"
        }
    },
    "replace" : {
        "paragonie/random_compat" : "2.*",
        "symfony/polyfill-ctype" : "*",
        "symfony/polyfill-iconv" : "*",
        "symfony/polyfill-php71" : "*",
        "symfony/polyfill-php70" : "*",
        "symfony/polyfill-php56" : "*"
    },
    "conflict" : {
        "symfony/symfony" : "*"
    },
    "minimum-stability" : "dev",
    "extra" : {
        "symfony" : {
            "allow-contrib" : false,
            "require" : "5.1.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "security-checker security:check": "script"
        }
    }
}
